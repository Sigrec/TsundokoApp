	<targets>
		<!-- <target name="logfile" xsi:type="File" fileName="file.txt" /> -->
		<target name="logconsole" xsi:type="Console" layout="[${environment:PROCESSOR_ARCHITECTURE}-${environment:OS}] ${longdate} | ${level:uppercase=true} > ${message:exceptionSeparator=\r\n:withException=true}"/>
	</targets>

	<rules>
		<logger name="*" minlevel="Info" writeTo="logconsole" />
		<logger name="*" minlevel="Debug" writeTo="logconsole" />
		<logger name="*" minlevel="Error" writeTo="logconsole" />
	</rules>
</nlog>
